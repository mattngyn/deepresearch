{
  "seed": 42,
  "out_dir": "checkpoints",
  "adapter_prefix": "rl-adapter",
  "model": {
    "base_model": "Qwen/Qwen2.5-VL-3B-Instruct",
    "lora_r": 32,
    "lora_alpha": 64,
    "lora_dropout": 0.05,
    "target_modules": [
      "q_proj",
      "k_proj",
      "v_proj",
      "o_proj",
      "gate_proj",
      "up_proj",
      "down_proj",
      "vision_language_merger.*"
    ],
    "min_pixels": 200704,
    "max_pixels": 200704
  },
  "training": {
    "lr": 2e-05,
    "epochs": 2,
    "mini_batch_size": 2,
    "episodes_per_batch": 16,
    "group_size": 6,
    "max_training_steps": 1000,
    "kl_beta": 0.001,
    "save_every_batches": 1
  },
  "actor": {
    "vllm_base_url": "http://localhost:8000/v1",
    "vllm_api_key": "token-abc123",
    "temperature": 0.7,
    "max_tokens": 2048,
    "max_steps_per_episode": 6,
    "parallel_episodes": 16,
    "system_prompt": "You are an expert agent. Complete the task efficiently."
  }
}
